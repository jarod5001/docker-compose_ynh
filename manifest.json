{
    "name": "Docker Compose",
    "id": "dockercompose",
    "packaging_format": 1,
    "description": {
        "en": "Compose is a tool for defining and running multi-container Docker applications",
        "fr": "Compose est un outil permettant de définir et d'exécuter des applications Docker multi-conteneurs"
    },
    "version": "2.3.4~ynh1",
    "url": "https://github.com/docker/compose",
    "upstream": {
        "license": "Apache-2.0 License",
        "website": "https://github.com/docker/compose",
        "demo": "https://github.com/docker/compose",
        "admindoc": "https://github.com/docker/compose",
        "userdoc": "https://github.com/docker/compose",
        "code": "https://github.com/docker/compose"
    },
    "license": "Apache-2.0 License",
    "maintainer": {
        "name": "jarod5001",
        "email": "john.doe@example.com"
    },
    "requirements": {
        "yunohost": ">= 4.3.0"
    },
    "multi_instance": false,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/example",
                "default": "/example"
            }
        ]
    }
}
